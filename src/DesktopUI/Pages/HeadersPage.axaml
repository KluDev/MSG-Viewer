<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModel="clr-namespace:DesktopUI.ViewModels"
             mc:Ignorable="d"
             x:Class="DesktopUI.Pages.HeadersPage"
             x:DataType="viewModel:HeadersPageViewModel">
    <Design.DataContext>
        <viewModel:HeadersPageViewModel />
    </Design.DataContext>

    <StackPanel Margin="15,15,0,0">
        <SelectableTextBlock Text="{Binding RawHeaders}" TextWrapping="Wrap" MaxWidth="1500"/>
    </StackPanel>

</UserControl>